{
    "type": "raptor-module",
    "name": "raptor/i18n",
    "version": "1.0",
    "description": "i18n client module",
    "homepage": "http://raptorjs.org",
    "authors": [
        {
            "name": "Phillip Gates-Idem",
            "email": "phillip.idem@gmail.com"
        }
    ],
    
    "raptor": {
        "dependencies": [
            "i18n.js",
            "Dictionaries.js",
            "Dictionary.js"
        ],

        "extensions": [
            {
                "name": "browser",
                "dependencies": [
                    "i18n_browser.js"
                ]
            },

            {
                "name": "rhino",
                "dependencies": [
                    "i18n_rhino.js"
                ]
            },

            {
                "name": "server",
                "dependencies": [
                    "i18n_server.js"
                ]
            },

            {
                "name": "node",
                "dependencies": [
                    "i18n_node.js"
                ]
            }
        ]
    }
}
