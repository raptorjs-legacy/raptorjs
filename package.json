        "test": "./test",
        "tools": "./tools"
    },
    "dependencies": {
        "uglify-js": "1.3.x",
        "mime": "~1.2.7",
        "sax": "~0.4.2",
        "esprima": "~0.9.9",
        "less": "~1.3.3",
        "xmldom": "~0.1.13",
        "optimist": "~0.3.5",
